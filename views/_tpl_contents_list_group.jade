div.row.content-item
  div.pull-left
    img.img-thumbnail(ng-src="https://133.11.240.228/mob/image/{{item.data.icon}}" width="50px" height="50px")
  div.pull-left
    h4.list-group-item-heading {{item.data.title}}
    p.list-group-item-text.list-detail {{item.data.body | limitTo: 60}}{{item.data.body.length > 60 ? '...' : ''}}
